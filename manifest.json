{
  "name": "Neptune's Pride Agent",
  "description": "Enhance NP Triton UI with intel.",
  "version": "1.7",
  "manifest_version": 3,
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://np.ironhelmet.com/*"
  ],
  "action": {},
  "web_accessible_resources": [
    {
      "resources": [
        "intel.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
		{
      "matches": [ "https://np.ironhelmet.com/*" ],
			"css": [ "intel.css" ]
		}
	],
  "background": {
    "service_worker": "background.js"
  }
}
